  "scripts": {
    "start": "craco start",
    "dev": "yarn start",
    "lint": "prettier --check src && eslint --cache ./src",
    "lint:fix": "prettier --write src && eslint --cache ./src --fix",
    "tauri": "tauri",
    "prebuild": "rimraf build",
    "profile": "webpack --profile --json > stats.json && webpack-bundle-analyzer stats.json dist/",
    "build": "yarn run lint:fix && GENERATE_SOURCEMAP=false yarn run build:app",
    "build:dev": "yarn run build:app",
    "build:tauri": "yarn tauri build --target aarch64-apple-darwin",
    "build:app": "INLINE_RUNTIME_CHUNK=false craco build",
    "test": "craco test --watchAll=false",
    "eject": "react-scripts eject",
    "extract-messages": "formatjs extract 'src/**/*.ts*' --ignore='**/*.d.ts' --out-file lang/en.json --id-interpolation-pattern '[sha512:contenthash:base64:6]'",
    "compile-messages": "formatjs compile-folder --ast lang compiled-lang",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not ie <= 99",
      "not android <= 4.4.4",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
